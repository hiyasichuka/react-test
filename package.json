{
  "name": "react-ts-test-example",
  "version": "1.0.0",
  "description": "A React test example with Playwright.",
  "author": "IBM",
  "license": "MIT",
  "scripts": {
    "prepare": "playwright install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "e2e:react": "playwright test --reporter=html test/react.e2e.spec.ts",
    "e2e:google": "playwright test --reporter=html test/google.e2e.spec.ts",
    "e2e:show-report": "playwright show-report",
    "serve": "serve ./build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "react-scripts": "^5.0.1",
    "serve": "^13.0.2",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
